    "v-money": "^0.8.1",
    "vue": "^2.0.8",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-the-mask": "^0.11.1"
  },
  "devDependencies": {
    "cross-env": "^5.1",
    "laravel-mix": "^5.0.1",
    "vue-template-compiler": "^2.6.14"
  }
